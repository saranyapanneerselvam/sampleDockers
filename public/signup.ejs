						<% } %>
						<div class="col-md-4 col-md-offset-4 col-sm-6 col-sm-offset-3 text-center">
							<h1 class="text-white">Sign up for free</h1>
							<div class="photo-form-wrapper clearfix">
								<form name="form" role="form" action="/api/v1/signup" method="post" >
									<input class="form-email" type="text" name="name" placeholder="Full Name" required>
									<input name="email" class="form-email" type="email" placeholder="Email Address" required>
									<input class="form-password" pattern=".{5,14}" type="password" name="password" placeholder="Password" required title="5 to 14 characters">
									<input class="form-email" type="text" name= "country" placeholder="Company Name" required>
									<input class="login-btn btn-filled" type="submit" value="Sign Up">
								</form>
							</div>
							<b class="text-white">Already have an account?</b>
							<a href="/api/v1/login" class="text-white">Login ➞</a><br>
							
